@page
@model IndexModel
@{
    ViewData["Title"] = "MyFavSong";
}

<link rel="stylesheet" type="text/css" href="~/css/site.css">
<script src="~/js/site.js" defer></script>

<body>
    <div class="content-container">
        <h1>Welcome to my Fav Song Website</h1>

